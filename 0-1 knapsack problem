#include <iostream>
using namespace std;
int max(int a,int b)
{
    if(a>b)
    return a;
    else
    return b;
}
int main() {
    int n,m;
    cin>>n>>m;
    int wt[n+1],p[n+1],k[n+1][m+1];
    int i,w,j,t1,t2;
    wt[0]=p[0]=0;
    for(i=1;i<=n;i++)
    cin>>wt[i];
    for(j=1;j<=n;j++)
    cin>>p[j];
    for(i=1;i<=n;i++)
    {
        for(j=i+1;j<=n;j++)
        {
            if(wt[i]>wt[j])
            {
                
                t1 = wt[i];
                wt[i] = wt[j];
                wt[j] = t1;
                
                 t1 = p[i];
                p[i] = p[j];
                p[j] = t1;
            }
        }
    }
	
	for(int i=0;i<=n;i++)
	{
	    for(int w=0;w<=m;w++)
	    {
	        if(i==0|| w==0)
	        {
	            k[i][w]=0;
	            
	        }
	        else if(wt[i]<=w)
	        k[i][w] = max(p[i] + k[i-1][w-wt[i]] ,  k[i-1][w]);
	        else
	        k[i][w] = k[i-1][w];
	        cout<<k[i][w]<<" ";
	    }
	    cout<<endl;
	   
	}
	return 0;
}
